@import "../../app.wxss";
page{
    background-color: #F8F8F8;
    font-size: 16px;
    font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}
.page__hd {
    padding: 10px;
}
.page__bd {
    padding-bottom: 10px;
}
.page__bd_spacing {
    padding-left: 15px;
    padding-right: 15px;
}

.page__ft{
    padding-bottom: 10px;
    text-align: center;
}

.weui-cells::after {
    border-bottom:0px;
}
.weui-cells::before {
    border-top:0px;
}
.weui-cell__bd{
    color:#888;
}
.pay{
    color:#F84E4C;
}
.black{
    color:#000;
}
.weui-cells{
    font-size:14px;
}
.container{
    width:94%;
    padding-left:3%;
    padding-right:3%;
    height: 100%;
}
.xian{
    border-bottom:1px solid #d9d9d9;
}
.clothes{
    width:30%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.single{
    width:20%;
    text-align: center;
}
.weui-cell{
   padding:10px 8px;
}
.cou{
  width: 80%;
  /* border:1px solid #d9d9d9; */
  border-radius: 50px;
  display: block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.middle_png{
    width:100%;
    height:10px;
}
.middle_png2{
    height:70px;
}
.p{
  width: 100%;
  padding-bottom: 10px;
  font-size: 12px;
  text-align: center;
}
.end{
  padding-top: 40px;
}
.button-sp-area{
    margin: 0 auto;
    padding-top: 15px;
    width: 60%;
}
.mini-btn{
    margin-right: 5px;
}
.page__bd_spacing{
    width: 94%;
    padding-left: 3%;
    padding-right: 3%;
    position: fixed;
    bottom: 0px;
}

button[type="primary"]{
    background-color:#ffa825;
}
.kong{
    width:100%;
    height:150px;
}